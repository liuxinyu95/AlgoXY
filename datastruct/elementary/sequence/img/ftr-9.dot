        {rank=same x8->x7->x6[style=invis]}

        mrearr[shape=point];
	x5[shape=circle, label="x[5]"];
	x4[shape=circle, label="x[4]"];
	x3[shape=circle, label="x[3]"];
        mrear->mrearr;
