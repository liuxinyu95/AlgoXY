digraph G {
	margin="0"
	root[shape=circle, label=""];
	front[shape=record, label=" x[9] "];
	mid[shape=circle, label=""];
	rear[shape=record, label=" x[2] | x[1] "];
	root->front;
	root->mid;
	root->rear;
